
public class YourSchedule
{
	public static void main(String[] args)
	{
		String PER1, PER2, PER3, T1, T2, T3, PER4, T4;
			PER1 = "APUSH";
			PER2 = "ECS";
			PER3 = "APLAN";
			T1 = "Van Over";
			T2 = "Mauro";
			T3 = "Gaughen";
			PER4 = "Unchedueled 4th";
			T4 = "N/A";
			
				System.out.println("Makena's Schedule");
				System.out.println("----------------------------------");
				System.out.println(" |   Per 1   | " +  PER1  +  " | " +  T1 + " |  ");
				System.out.println(" |   Per 2   | " +  PER2  +  "   | " +  T2 + "    |  ");
				System.out.println(" |   Per 3   | " +  PER3  +  " | " +  T3 + "  |  ");
				System.out.println(" |   Per 4    |" +  PER4  +  " | " +  T4 + "  |  ");
		
		
		
	}
}