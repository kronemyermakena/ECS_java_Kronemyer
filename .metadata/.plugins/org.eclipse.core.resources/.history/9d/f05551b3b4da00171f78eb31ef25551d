
import java.util.Scanner;

	public class UserInputofData
{
		public static void main(String[] args)
	{
			Scanner keyboard = new Scanner(System.in);
			
				String Fname, Lname, login; 
				int age, GPA, ID, grade;
					
					System.out.println("First Name: "); System.out.println("Last Name: ");
					Fname = keyboard.next();				Lname = keyboard.next();
					
					
					System.out.println("Grade (9-12): ");
					grade = keyboard.nextInt();
							
					System.out.println("Student ID: ");
					ID = keyboard.nextInt();
							
					System.out.println("Login: ");
					login = keyboard.next();
							
					System.out.println("GPA (0.0 - 4.0): ");
					GPA = keyboard.nextInt();
							
					
					
					
					
					
					
					
					
					
					keyboard.close();
			
	}

}
	
 