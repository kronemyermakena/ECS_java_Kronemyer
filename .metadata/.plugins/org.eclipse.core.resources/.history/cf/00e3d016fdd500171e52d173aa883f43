
public class YourInitials
{
	public static void main(String[] args)
	{
		System.out.println("M     M  M     M");	
		System.out.println("M M M M  MM   MM");
		System.out.println("M M M M  MM   MM");
		System.out.println("M  M  M  M  M  M"); 
		System.out.println("M     M  M     M");
		System.out.println("M     M  M     M");
		System.out.println("M     M  M     M");
	
		



	}
}




