
public class YourInitials
{
	public static void main(String[] args)
	{
		System.out.println("M     M  M     M  K   K");	
		System.out.println("MM   MM  MM   MM  K  K");
		System.out.println("MM   MM  MM   MM  K K");
		System.out.println("M  M  M  M  M  M  KK"); 
		System.out.println("M     M  M     M  K K");
		System.out.println("M     M  M     M  K  K");
		System.out.println("M     M  M     M  K   K");
	
		



	}
}
 



