
public class YourInitials
{
	public static void main(String[] args)
	{
		System.out.println("M    M");	
		System.out.println("MM MM M");
		System.out.println("MM MM M");
		System.out.println("M M   M"); 
		System.out.println("M     M");
		System.out.println("M     M");
		System.out.println("M     M");
		
		



	}
}




